  "devDependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.200",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.52.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.0.3",
