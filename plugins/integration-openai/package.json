    "openai": "^4.24.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/types": "^7.23.6",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.56.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
