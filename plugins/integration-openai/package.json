{
  "name": "@amplication/plugin-integrate-openai",
  "version": "2.0.2",
  "description": "Add a module and a service to use OpenAI API to Amplication generated code",
  "main": "dist/index.js",
  "nx": {},
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack",
    "prebuild": "rimraf dist"
  },
  "author": "Yuval Hazaz",
  "license": "Apache-2.0",
  "dependencies": {
    "@amplication/code-gen-types": "^2.0.21",
