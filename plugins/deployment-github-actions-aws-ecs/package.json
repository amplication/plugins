{
  "name": "@amplication/plugin-deployment-github-actions-aws-ecs",
  "version": "0.0.2",
  "description": "Adds a GitHub Actions workflow for the packaging to AWS Elastic Container Registry (ECR) and publishing to AWS Elastic Container Service (ECS). This plugin requires the ECR & ECS to be present and passed as configuration.",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "webpack --watch",
    "build": "webpack",
    "prebuild": "rimraf dist",
    "format:write": "prettier --write \"**/*.{js,ts}\""
  },
  "author": "Levi van Noort <levi@amplication.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@amplication/code-gen-types": "^2.0.17",
    "@amplication/code-gen-utils": "^0.0.7",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/parser": "^7.20.7",
    "@babel/types": "^7.20.7",
    "@types/lodash": "^4.14.199",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.21.0",
    "jest-mock-extended": "^3.0.1",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.3",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
